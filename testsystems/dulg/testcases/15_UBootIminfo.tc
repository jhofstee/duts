# -*- Mode: tcl -*-
# Check 'iminfo' command
#

duts_tc UBootIminfoHelp {
	Type u-boot
	Commands {
		"help iminfo" ".*"
	}
}

duts_tc UBootIminfo {
	Type u-boot
	Pre {
		"setenv bootfile $CFG_LINUX_KERNEL" ".*"
	}
	Commands {
		"tftp ${ram_ws} ${bootfile}" "Bytes transferred"
		"imi ${ram_ws}" "Verifying Checksum ... OK"
	}
}
