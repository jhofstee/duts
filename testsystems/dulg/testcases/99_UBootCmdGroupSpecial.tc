duts_tc UBootI2cHelp {
	Type u-boot
	Requires i2c
	Commands {
		"help iprobe" ".*"
		"help icrc32" ".*"
		"help imw" ".*"
		"help inm" ".*"
		"help imm" ".*"
		"help imd" ".*"
		"help iloop" ".*"
	}
}

duts_tc UBootIdeHelp {
	Type u-boot
	Requires ide
	Commands {
		"help ide" ".*"
	}
}

duts_tc UBootDiskbootHelp {
	Type u-boot
	Requires ide
	Commands {
		"help disk" ".*"
	}
}
