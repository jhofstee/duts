#
# yucca flash layout:
#
# Bank #1 - 512 KB in 8 Sectors
#
#   FFF00000 - FFF7FFFF
#
#
# Bank #2 - 512 KB in 8 Sectors
#
#   FFF80000 - FFFF0000
#
#   layout:
#   FFFA0000 - FFFAFFFF environment
#   FFFB0000 - FFFFFFFF U-Boot
#
#
# Bank #3 - 4096 KB in 71 Sectors
#
#   E7C00000 - E7FFEFFF
#
#   layout:
#   E7C00000 - E7D3FFFF kernel (20 sectors)
#   E7D40000 - E7E9FFFF ramdisk (22 sectors)
#
#
duts_device yucca {
	Features {
		linux_in_nor
	}
	Vars {
		CFG_ROOTPATH		"/opt/eldk-4.2/ppc_4xx/"

		CFG_MTD_ERASE_DEVICE	"mtd6"

		CFG_FLASH_TST_START	0xFFF00000
		CFG_FLASH_TST_END	0xFFF7FFFF
		CFG_FLASH_ERASE_START	0xFFF00000
		CFG_FLASH_ERASE_END	0xFFF5FFFF

		CFG_FLASH_TST_SIZE	0x40000

		CFG_FLASH_PROT1_START	0xFFFA0000
		CFG_FLASH_PROT1_END	0xFFFFFFFF

		CFG_FLASH_ERASE_SECT	"1:0-1"
		CFG_FLASH_ERASE_BANK	"1"
		CFG_FLASH_TST_SECT	"1:0"

		CFG_KERNEL_START	0xE7C00000
		CFG_KERNEL_END		0xE7D3FFFF
		CFG_RAMDISK_START	0xE7D40000
		CFG_RAMDISK_END		0xE7ECFFFF

		BUILD_TARGET		"yucca"
		BUILD_ARCH		"ppc"
		BUILD_CROSS		"ppc_4xx-"
	}
}
