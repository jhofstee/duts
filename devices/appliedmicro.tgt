		CFG_FLASH_ERASE_END	0xff95ffff

		CFG_MTD_ERASE_DEVICE	"mtd2"

		CFG_FLASH_PROT1_START	0xfff80000
		CFG_FLASH_PROT1_END	0xffffffff

		CFG_FLASH_ERASE_SECT	"1:72-73"
		CFG_FLASH_ERASE_BANK	"1"
		CFG_FLASH_TST_SECT	"1:72"

		CFG_KERNEL_START	0xff000000
		CFG_KERNEL_END		0xff17ffff
		CFG_RAMDISK_START	0xff180000
		CFG_RAMDISK_END		0xff37ffff

		BUILD_TARGET		"katmai"
		BUILD_ARCH		"ppc"
		BUILD_CROSS		"ppc_4xx-"
	}
}

