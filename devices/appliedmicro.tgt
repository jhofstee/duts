#
# Definitions for AppliedMicro boards
#

duts_device arches {
	Features {
		 i2c
		 dtt
		 fdt
		 linux_in_nor
	}
	Vars {
		CFG_ROOTPATH		"/opt/eldk-4.2/ppc_4xxFP/"
		CFG_MTD_ERASE_DEVICE	"mtd3"

		CFG_FLASH_TST_START	0xFF900000
		CFG_FLASH_TST_END	0xFF97FFFF
		CFG_FLASH_ERASE_START	0xFF900000
		CFG_FLASH_ERASE_END	0xFF95FFFF

		CFG_FLASH_TST_SIZE	0x40000

		CFG_FLASH_PROT1_START	0xFFF60000
		CFG_FLASH_PROT1_END	0xFFFFFFFF

		CFG_FLASH_ERASE_SECT	"1:220-221"
		CFG_FLASH_ERASE_BANK	"1"
		CFG_FLASH_TST_SECT	"1:220"

		CFG_KERNEL_START	0xFE000000
		CFG_KERNEL_END		0xFE17FFFF

		CFG_FDT_START		0xFE1E0000
		CFG_FDT_END		0xFE1FFFFF
		CFG_FDT_ADDR_R		0x00b00000

		CFG_RAMDISK_START	0xFE200000
		CFG_RAMDISK_END		0xFE3FFFFF

		BUILD_TARGET		"canyonlands"
		BUILD_TARGET_PREFIX	"44x"
		BUILD_ARCH		"powerpc"
		BUILD_CROSS		"ppc_4xxFP-"
	}
}

